{
  "name": "xtal-fig",
  "version": "0.0.21",
  "description": "Turn css for basic shapes into semantic html",
  "main": "xtal-fig-diamond.js",
  "module": "xtal-fig-diamond.js",
  "keywords": [
    "web-components",
    "custom-elements",
    "flowcharts",
    "shapes"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-check": "package-check",
    "serve": "node node_modules/xtal-test/serve.js",
    "doc": "custom-elements-manifest analyze"
  },
  "exports": {
    ".": "./index.js",
    "./xtal-fig-parallelogram.js": "./xtal-fig-parallelogram.js",
    "./xtal-fig-diamond.js": "./xtal-fig-diamond.js",
    "./xtal-fig-document.js": "./xtal-fig-document.js",
    "./xtal-fig-db-cylinder.js": "./xtal-fig-db-cylinder.js"
  },
  "files": [
    "xtal-*.*",
    "index.*",
    "types.d.ts",
    "custom-elements.json"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "trans-render": "0.0.445",
    "xtal-element": "0.0.458",
    "slot-bot": "0.0.16",
    "be-ferried": "0.0.23"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.54",
    "@custom-elements-manifest/analyzer": "0.5.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-fig.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-fig/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-fig#readme",
  "customElements": "custom-elements.json"
}
