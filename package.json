{
  "name": "xtal-fig",
  "version": "0.0.25",
  "description": "Turn css for basic shapes into semantic html",
  "main": "xtal-fig-diamond.js",
  "module": "xtal-fig-diamond.js",
  "keywords": [
    "web-components",
    "custom-elements",
    "flowcharts",
    "shapes"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-check": "package-check",
    "serve": "node node_modules/may-it-serve/serve.js",
    "safari": "npx playwright wk http://localhost:3030",
    "update": "ncu -u && npm install",
    "build": "powershell -command \"node parallelogram/def.mjs | out-file  -encoding  ASCII parallelogram/def.html\""
  },
  "exports": {},
  "files": [
    "*.*",
    "diamond/*.*",
    "parallelogram/*.*",
    "cylinder/*.*",
    "demo/*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "be-hive": "0.0.132",
    "be-importing": "0.0.73",
    "blow-dry": "0.0.6",
    "trans-render": "0.0.731",
    "xtal-element": "0.0.590"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "may-it-serve": "0.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-fig.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-fig/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-fig#readme",
  "customElements": "custom-elements.json"
}
