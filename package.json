{
  "name": "xtal-fig",
  "version": "0.0.22",
  "description": "Turn css for basic shapes into semantic html",
  "main": "xtal-fig-diamond.js",
  "module": "xtal-fig-diamond.js",
  "keywords": [
    "web-components",
    "custom-elements",
    "flowcharts",
    "shapes"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-check": "package-check",
    "serve": "node node_modules/may-it-serve/serve.js",
    "doc": "custom-elements-manifest analyze",
    "build": "copy xfdb-config.js xfdb-config.mjs && node xfdb-config.mjs -dfn > xtal-fig-db-cylinder.dfn.json && node xfdb-config.mjs -js > xtal-fig-db-cylinder.js && node xfdb-config.mjs -html > xtal-fig-db-cylinder.html",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "*-config.js"
  },
  "exports": {
    ".": "./index.js",
    "./xtal-fig-parallelogram.js": "./xtal-fig-parallelogram.js",
    "./xtal-fig-diamond.js": "./xtal-fig-diamond.js",
    "./xtal-fig-document.js": "./xtal-fig-document.js",
    "./xtal-fig-db-cylinder.js": "./xtal-fig-db-cylinder.js"
  },
  "files": [
    "*.*"
  ],
  "types": "types.d.ts",
  "dependencies": {
    "trans-render": "0.0.553",
    "xtal-element": "0.0.496",
    "be-definitive": "0.0.55",
    "be-active": "0.0.33",
    "be-ferried": "0.0.39"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "may-it-serve": "0.0.2",
    "@custom-elements-manifest/analyzer": "0.6.4",
    "may-it-be": "0.0.163",
    "npm-watch": "0.11.0",
    "be-importing": "0.0.26"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-fig.git"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-fig/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-fig#readme",
  "customElements": "custom-elements.json"
}
